{
  "name": "twitter-list-images",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "repository": "git@github.com:tomacheese/twitter-list-images.git",
  "author": "Tomachi",
  "private": true,
  "scripts": {
    "build": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "clean": "rimraf dist output",
    "generate-interface": "ts-node ./src/generate-type-interface.ts",
    "generate-schema": "typescript-json-schema --required tsconfig.json Configuration -o ../schema/Configuration.json",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@book000/node-utils": "1.2.38",
    "@types/node": "18.15.13",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vercel/ncc": "0.36.1",
    "axios": "1.3.6",
    "cycle": "1.0.3",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "genson-js": "0.0.8",
    "json-schema-to-typescript": "12.0.0",
    "logform": "2.5.1",
    "otplib": "12.0.1",
    "prettier": "2.8.7",
    "puppeteer-core": "19.9.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "twitter-d": "0.6.0",
    "typescript": "5.0.4",
    "typescript-json-schema": "0.56.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1",
    "yarn-run-all": "3.1.1"
  },
  "dependencies": {
    "@fastify/cors": "8.2.1",
    "fastify": "4.15.0"
  }
}